{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true,
		"jest": true,
		"node": true
	},
	"rules": {
		"no-unused-vars": "warn",
		"no-console": "off"
	},
	"plugins": ["import"],
	"extends": ["eslint:recommended", "prettier"]
}
